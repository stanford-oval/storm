# Storm UI Environment Variables
# Copy this file to .env.local and fill in your API keys

# Backend API URL
NEXT_PUBLIC_API_URL=http://localhost:8000/api

# Language Model API Keys
# OpenAI
NEXT_PUBLIC_OPENAI_API_KEY=
NEXT_PUBLIC_OPENAI_API_TYPE=openai
NEXT_PUBLIC_OPENAI_BASE_URL=

# Azure OpenAI (if using Azure)
NEXT_PUBLIC_AZURE_API_KEY=
NEXT_PUBLIC_AZURE_API_BASE=
NEXT_PUBLIC_AZURE_API_VERSION=2024-02-01
NEXT_PUBLIC_AZURE_DEPLOYMENT_NAME=

# Anthropic (Claude)
NEXT_PUBLIC_ANTHROPIC_API_KEY=

# Google (Gemini)
NEXT_PUBLIC_GOOGLE_API_KEY=

# Groq
NEXT_PUBLIC_GROQ_API_KEY=

# Ollama (for local models)
NEXT_PUBLIC_OLLAMA_BASE_URL=http://localhost:11434

# Retriever/Search API Keys
# Bing Search
NEXT_PUBLIC_BING_SEARCH_API_KEY=

# You.com
NEXT_PUBLIC_YDC_API_KEY=

# Serper (Google Search)
NEXT_PUBLIC_SERPER_API_KEY=

# Brave Search
NEXT_PUBLIC_BRAVE_API_KEY=

# Tavily Search
NEXT_PUBLIC_TAVILY_API_KEY=

# DuckDuckGo (no API key needed)
NEXT_PUBLIC_USE_DUCKDUCKGO=true

# Vector Database (Qdrant)
NEXT_PUBLIC_QDRANT_API_KEY=
NEXT_PUBLIC_QDRANT_URL=

# WebSocket Configuration
NEXT_PUBLIC_WS_URL=ws://localhost:8000/ws

# Feature Flags
NEXT_PUBLIC_ENABLE_COSTORM=false
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_DEBUG_MODE=true

# Default Configuration
NEXT_PUBLIC_DEFAULT_LLM_PROVIDER=openai
NEXT_PUBLIC_DEFAULT_LLM_MODEL=gpt-4
NEXT_PUBLIC_DEFAULT_RETRIEVER_TYPE=bing
NEXT_PUBLIC_DEFAULT_MAX_TOKENS=4000
NEXT_PUBLIC_DEFAULT_TEMPERATURE=0.7
NEXT_PUBLIC_DEFAULT_MAX_SEARCH_RESULTS=10

# Rate Limiting
NEXT_PUBLIC_MAX_CONCURRENT_PIPELINES=3
NEXT_PUBLIC_MAX_REQUESTS_PER_MINUTE=60

# Storage
NEXT_PUBLIC_ENABLE_LOCAL_STORAGE=true
NEXT_PUBLIC_STORAGE_PREFIX=storm_ui_